SERV_DB=192.168.0.11
USER=zeusDBA
PASSWORD=123456
DB_ZEUS_NAME=zeus
DB_SISAC_NAME=Sisac
PUERTO=3306
MAX_TIEMPO_VENTA=2
TIEMPO_ELIMINACION=60
TIEMPO_CHEQUEO_PREVENTA=1
MIN_HILOS=20
MAX_HILOS=30
DELTA_HILOS=5
SERVIDOR_PUERTO=3000
SERVIDOR_PUERTO_ADMIN=4200
COLA_DE_ESPERA=128
MAX_ADMIN_CON=100
TIEMPO_ESPERA=250000
DIRECTORIO_REGISTROS=.
DIRECTORIO_PIDOF=.
DIRECTORIO_RESPALDOS=.
NIVEL_REGISTRO_MENSAJES=3
NIVEL_REGISTRO_CLIENTES=3
NIVEL_REGISTRO_ADMIN=3
AUTENTIFICACION=14B4NC43514M3J0R
TIEMPO_VERIFICAR_CONEXION=5
TIEMPO_DESCONECTAR=10
DIASINFORMACION=0
RUTA_ACTUALIZACION_TAQUILLA=/tmp/infov/infov
# Final del Archivo
